{
  "permissions": {
    "allow": [
      "Skill(gitpro)"
    ],
    "defaultMode": "bypassPermissions"
  },
  "hooks": {
    "PreCompact": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/beads-inject.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Skill",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/pre-gitpro.sh",
            "timeout": 120
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/block-db-commands.sh",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/git-guard.sh",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/dev-server-guard.sh",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "mcp__acp__Bash",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/block-db-commands.sh",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/git-guard.sh",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/dev-server-guard.sh",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/block-console-log.sh",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/block-console-log.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/beads-inject.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/architect_enforcer.sh",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "~/.claude/hooks/active-now-reminder.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/bead-comment-reminder.sh",
            "timeout": 5
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh",
    "padding": 0
  },
  "enabledPlugins": {
    "frontend-design@claude-code-plugins": true,
    "typescript-lsp@claude-plugins-official": true
  }
}
